<?xml version="1.0"?>
<configuration>
	<appSettings>
		<add key="SpellChecker.IgnoreAllCapsWords" value="True"/>
		<add key="SpellChecker.IgnoreHtml" value="True"/>
		<add key="SpellChecker.IgnoreWordsWithDigits" value="False"/>
		<add key="SpellChecker.MaxSuggestions" value="25"/>
		<add key="SpellChecker.Dictionary.DictionaryFolder" value="..\\dic"/>
		<add key="SpellChecker.Dictionary.DictionaryFile" value=""/>
		<add key="SpellChecker.Dictionary.UserFile" value=""/>
	</appSettings>
	<system.diagnostics>
		<switches>
			<add name="NetSpell.SpellChecker" value="3" />
		</switches>
		<trace autoflush="true" indentsize="2">
			<listeners>
				<add name="TextEditor" type="System.Diagnostics.TextWriterTraceListener" initializeData="TextEditor.log" />
			</listeners>
		</trace>
	</system.diagnostics>
</configuration>
